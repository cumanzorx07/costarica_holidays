{
    "name": "cumanzorx07/costarica_holidays",
    "type": "library",
    "license": "MIT",
    "description": "Wrapper para Carbon con los d√≠as de feriados celebrados en Costa Rica",
    "require": {
        "php": "^7.3",
        "myclabs/php-enum": "^1.7",
        "nesbot/carbon": "^2.29"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "CRHolidays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CRHolidays\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
